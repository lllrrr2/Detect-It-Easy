init("image","Multiple-image Network Graphics (.MNG)")
function detect(){if(Binary.getSize()>=64){if(Binary.compare("8A'MNG\r\n'1A0A........'MHDR'")){bDetected=true
if(Binary.isVerbose()){sOption(Binary.read_uint32(16,_BE)+"x"+Binary.read_uint32(20,_BE))
sOption("frames:"+Binary.read_uint32(24,_BE))}}}return result()}