function _isLangPresent(e){return _isResultPresent("language",e)||_isResultPresent("~language",e)}function _isLangDetected(){return _getNumberOfResults("language")||_getNumberOfResults("~language")}function _setLang(e,n,s){if(n==true||n==undefined){if(_isLangPresent("C/C++")&&(e=="C"||e=="C++")){_removeResult("language","C/C++")}if(e=="C/C++"&&(_isLangPresent("C")||_isLangPresent("C++"))){return null}if(_isLangPresent("C")&&e!=="C"){_removeResult("language","C")}if(_isLangPresent("C++")&&e!=="C++"){_removeResult("language","C++")}if(_isLangDetected()&&(e=="C/C++"||e=="C++"||e=="C")){return null}if(File.isVerbose()&&!_isLangPresent(e)){var t=s=="HEUR"
_setResult((t?"~":"")+"language",e,s&&!t?s:"","")}}}