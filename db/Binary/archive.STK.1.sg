// This Script supports following formats: STK/ITK (DEV7 only)

// https://wiki.scummvm.org/index.php?title=Gob/DEV7_Information#STK21_File_Structure
init("archive", "STK");

function detect() {
    if (Binary.compare("'STK2.1'")) {
        sVersion = "STK21";
        bDetected = true;
    }

    return result();
}