// Detect It Easy: detection rule file
includeScript("FPC");

init("library", "LCL");

function detect() {
    if (bFPC) {
        var verInfoOffset = PE.findString(0, PE.isOverlayPresent() ? PE.getOverlayOffset() : PE.getSize(), "LCLVersion");
        if (verInfoOffset != -1) {
            sVersion = PE.getString(verInfoOffset + 12);
            bDetected = true;
        }
    }

    return result();
}